{
    "name": "sashalenz/dinstar-api",
    "description": "Dinstar New API",
    "keywords": [
        "sashalenz",
        "laravel",
        "dinstar-api",
        "dinstar"
    ],
    "homepage": "https://github.com/sashalenz/dinstar-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksandr Petrovskyi",
            "email": "sashalenz@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.11",
        "illuminate/contracts": "^9.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1"
    },
    "autoload": {
        "psr-4": {
            "Sashalenz\\DinstarApi\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sashalenz\\DinstarApi\\DinstarApiServiceProvider"
            ],
            "aliases": {
                "DinstarApi": "Sashalenz\\DinstarApi\\Facades\\DinstarApi"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
